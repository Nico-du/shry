				if((Number(hszsbl) > 1000 || Number(hszsbl) < 0) ){ Alert.show("换算比例值必须在0~1000之间！",CommonXMLData.Alert_Title); return; }
				if((Number(hsdlhzj) > 10000 || Number(hsdlhzj) < 0 ) ){ Alert.show("换算值必须在0~10000之间！",CommonXMLData.Alert_Title); return; }
				getChartDataAndAddChart(Number(hszsbl),Number(hsdlhzj));
			}
			
