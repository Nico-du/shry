			//性能数据转换后导出
			public function exportHSJG_clickHandler():void{
				if(hsCbx.selectedItem == null ){ return; }
				var sLxdid:String = hsCbx.selectedItem.lxdid;
				
				var variables:URLVariables = new URLVariables();
				variables.jdbcSql = "";
				variables.dataid = this.dataId;
				variables.dataid2 = sLxdid;
				variables.exportType = "ZCXNHS";
				variables.extraData = JSON.encode(chartDataList); ;
				
				var u:URLRequest = new URLRequest("/mxAsset/servlet/ExportAllExcel");
				u.data = variables; 
				u.method = URLRequestMethod.POST; 
				navigateToURL(u,"_self");		
			}
			
