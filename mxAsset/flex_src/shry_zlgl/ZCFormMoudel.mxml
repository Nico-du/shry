	<mx:Script>
		<![CDATA[
			import asset_bfdj.ProcessMethods;
			
			import cn.cnsasfram.entity.DoResult;
			import cn.cnsasfram.entity.enum.Errors;
			import cn.cnsasfram.helper.ASCommonHelper;
			import cn.cnsasfram.helper.ASDateHelper;
			import cn.cnsasfram.helper.ASJsonHelper;
			import cn.cnsasfram.helper.ASStringHelper;
			import cn.cnspagemoduel.chartline.LinueChartModel;
			import cn.cnspagemoduel.chartline.LinueChartMultiModel;
			
			import com.adobe.serialization.json.JSON;
			
			import flex.util.CommonMethod;
			import flex.util.CommonUIMethod;
			
			import mx.collections.ArrayCollection;
			import mx.controls.Alert;
			import mx.core.Application;
			import mx.core.FlexGlobals;
			import mx.core.UIComponent;
			import mx.events.CloseEvent;
			import mx.managers.PopUpManager;
			import mx.rpc.events.ResultEvent;
			import mx.utils.ObjectUtil;
			import mx.validators.Validator;
			//所有按钮高度，工具栏
			[Bindable]
			protected var aboutheight:Number=20;
			
			//页面标题
			[Bindable]
			public var pageTitle:String="总成数据";
			
			//数据标题
			[Bindable]
			public var dataTitles:String="";
			
			//数据ID
			public var dataId:String="";
			//默认操作实体名称
			public var tempObjBeanName:String="SHRY_ZC_DATA";
			//视图名称
			public var tempObjViewName:String="SHRY_ZC_VIEW";
			//主键
			public var idColumnName:String = "zcid";
			//保存数据
			public var dataArray:ArrayCollection=null;
			//页面是否加载权限控制
			public var pageIsLoadPoer:Boolean=true;
			//页面初始化是否加载数据
			public var pageIsLoadData:Boolean=true;
			//父窗口
			public var parentPage:ZCGridMoudel=null;
			//临时存储方法
			public var tempFunction:Function=null;
			//图表数据
			var chartDataList:ArrayCollection = new ArrayCollection();
			//页面初始化调用
			public function initPage():void{
				if(true){
					beforeInitPage();
				}
			}
			
			//初始化页面之前调用
			public function beforeInitPage():void{
				this.width = Application.application.width * 0.8;
				this.height = Application.application.height * 0.8;
				
				if(pageIsLoadPoer){
					loadUserPower();
				}
			}
			
			//分析权限
			public function loadUserPower():void{
				//判断读取权限
				
				//判断写入权限				
				//ASCommonHelper.controlVisible(this.saveAndNewAction,false);
				//ASCommonHelper.controlVisible(this.newAction,false);
				//ASCommonHelper.controlVisible(this.removeAndCloseAction,false);
				
				//审核界面 只能修改实际数量
				/* if(parentPage.parentPage.isSHPage){
					CommonMethod.setEnable(nvPage1,false);
					shbfsl.enabled = true; shbfsl.editable = true;
					sjbfsl.enabled = true; sjbfsl.editable = true;
					lggd.enabled = true; lggd.editable = true;
				} */
				ProcessMethods.changeCNSTModuelState(this,parentPage.newAction.visible,parentPage.editAction.visible ,parentPage.removeAction.visible,this.dataId=="");
			}
			
			//初始化页面之后调用
			public function afterInitPage():void{
				if(pageIsLoadData){
					loadData();
				}
				loadPictures();
			}
			
			//页面关闭时调用
			public function closePage():void{
				if(true){
					beforeClosePage();
				}
				PopUpManager.removePopUp(this);
				if(true){
					afterClosePage();
				}
			}
			
			//关闭页面之前调用
			protected function beforeClosePage():void{
			}
			
			//关闭页面之后调用
			protected function afterClosePage():void{
				if(this.parentPage!=null){
					this.parentPage.loadData();
				}
				this.pageIsLoadData=false;
			}
			
			//保存事件
			protected function saveActionClick():void{
				this.saveData();
				this.tempFunction=this.loadData;
			}
			
			//保存并新建事件
			protected function saveAndNewActionClick():void{
				this.saveData();
				this.tempFunction=this.resetData;
			}
			
			//新建事件
			protected function newActionClick():void{
				this.resetData();
			}
			
			//保存并关闭事件
			protected function saveAndCloseActionClick():void{
				this.saveData();
				this.tempFunction=this.closePage;
			}
			
			//删除并关闭事件
			protected function removeAndCloseActionClick():void{
				this.removeData();
				this.tempFunction=this.closePage;
			}
			
			//关闭事件
			protected function closeActionClick():void{
				this.closePage();
			}
			
			//加载数据
			public function loadData():void{
				this.dataSearchAction();
			}
			
			//保存数据
			public function saveData():void{
				this.dataSaveOrUpdateAction();
			}
			
			//删除数据
			public function removeData():void{
				if(ASStringHelper.isNullOrEmpty(this.dataId)){
					return;
				}
				Alert.show("您确认要删当前数据吗？","系统提示",Alert.OK|Alert.CANCEL,Application.application as Sprite,dataRemoveAction,null,Alert.CANCEL);
			}
			
			//重置数据
			protected function resetData():void{
				this.title = "新建总成数据";
				this.dataId="";
				this.dataArray=new ArrayCollection();
				this.dataArray.addItem(new Object());
				this.afterLoadData();
				this.errormsgbox.removeAllChildren();
				
			}
			
			//加载搜索条件
			protected function getDataSeacrchCondition():String{
				var condition:Object=new Object();
				condition.s_zcid_string_eq=this.dataId;
				var conditionJson:String=JSON.encode(condition);
				return conditionJson;
			}
			//加载相关信息
			protected function dataSearchAction():void{
				if(ASStringHelper.isNullOrEmpty(this.dataId)){
					this.resetData();
					return;
				}
				this.title = "编辑总成数据";
				var strcondition:String=getDataSeacrchCondition();
				if(ASStringHelper.isNullOrEmpty(strcondition)){
					return;
				}
				this.AllAssetServiceRmt.GetAllAsset(tempObjViewName,20,1,strcondition);
				this.AllAssetServiceRmt.addEventListener(ResultEvent.RESULT,dataSearchActionBack);
			}
			//加载相关信息回调方法
			protected function dataSearchActionBack(rmtResult:ResultEvent):void{
				this.AllAssetServiceRmt.removeEventListener(ResultEvent.RESULT,dataSearchActionBack);
				var objResult:Object=rmtResult.result;
				var strresult:String=objResult.toString();
				var jsonObj:Object=JSON.decode(strresult,true);
				var itemtotal:Object=jsonObj.itemtotal;
				var othermsg:Object=jsonObj.othermsg;
				var items:Object=jsonObj.items;
				if(items==null){
					this.resultmsg.text="未请求到相关数据"
					this.resultmsg.alpha=1;
					this.fadeEffect.stop();
					this.fadeEffect.play(null,false);
					return;
				}
				this.dataArray=new ArrayCollection(items as Array);
				this.afterLoadData();
				this.loadUserPower();
			}
			//加载数据成功之后调用
			protected function afterLoadData():void{
				if(this.dataArray==null || this.dataArray.length<1){
					return;
				}
				var tempObj:Object=this.dataArray.getItemAt(0);
				this.dataTitles=tempObj["xh"];
				this.xh.text=tempObj["xh"];
				
				this.fyxh.text=tempObj["fyxh"];
				this.fyxh.data=tempObj["fyid"];
				this.djxh.text=tempObj["djid"];
		//		this.djxh.data=tempObj["djid"];
				
				this.jqfs.text=tempObj["jqfs"];
				this.wxcc.text=tempObj["wxcc"];
				this.syzt.text=tempObj["syzt"];
				this.zl.text=tempObj["zl"];
				this.fslx.text=tempObj["fslx"];
				this.sycx.text=tempObj["sycx"];
				
				this.memo.text=tempObj["memo"];
				
				this.dqdlhzjinput.text=tempObj["dlhzj"];
				this.hsdlhzjinput.text=tempObj["dlhzj"];
				
				/* jqfs.data=tempObj["assettablename"];
				ypsm.data=tempObj["assetidcolumn"];
				ypfb.data=tempObj["assetid"]; */
				
				if(!CommonMethod.isNullOrWhitespace(this.memo.text) && memolkb.visible){	setMemoVisable();}
				this.UPDATEDATE.text=tempObj[this.UPDATEDATE.id.toLowerCase()];
				this.UPDATEUSER.text=tempObj[this.UPDATEUSER.id.toLowerCase()+"name"];
				this.INPUTDATE.text=tempObj[this.INPUTDATE.id.toLowerCase()];
				this.INPUTUSER.text=tempObj[this.INPUTUSER.id.toLowerCase()+"name"];
			}
			
			//保存数据之前验证数据
			protected function beforeSaveData():Boolean{
				this.errormsgbox.removeAllChildren();
				var errorMsg:String="";
				var isCheckSuccess:Boolean=true;
				if(ASStringHelper.isNullOrEmpty(this.xh.text)){
					errorMsg="[总成型号]不允许为空";
					showCheckErrors(errorMsg);
					isCheckSuccess=false;
				}
				if(ASStringHelper.isNullOrEmpty(this.fyxh.data+"")){
					errorMsg="[风叶型号]不允许为空";
					showCheckErrors(errorMsg);
					isCheckSuccess=false;
				}
				if(ASStringHelper.isNullOrEmpty(this.djxh.data+"")){
					errorMsg="[电机型号]不允许为空";
					showCheckErrors(errorMsg);
					isCheckSuccess=false;
				}
				if(ASStringHelper.isNullOrEmpty(this.jqfs.text)){
					errorMsg="[进气方式]不允许为空";
					showCheckErrors(errorMsg);
					isCheckSuccess=false;
				}
				if(ASStringHelper.isNullOrEmpty(this.fslx.text)){
					errorMsg="[风扇类型]不允许为空";
					showCheckErrors(errorMsg);
					isCheckSuccess=false;
				}
				//执行编号重复验证
				if(isCheckSuccess){
					CommonMethod.validateWFBH(this.xh.text,"ZCID","XH",this.tempObjBeanName,this.dataId,this,beforeSaveDataBack);
					return false;
				}
				return isCheckSuccess;
			}
			
			//验证编号重复的回调函数 
			public function beforeSaveDataBack(isbhcf:Boolean = false):void{
				if(isbhcf){
					var errorMsg:String="[总成型号]重复,请重新输入";
					showCheckErrors(errorMsg);
				//	xh.text = CommonMethod.genernateIncreaseWFBH(xh.text);
					return;
				}
				dataSaveOrUpdate();
			}
			
			//显示错误信息
			protected function showCheckErrors(errorMsg:String):void{
				if(ASStringHelper.isNullOrEmpty(errorMsg)){
					return;
				}
				var tempTextInput:TextInput=new TextInput();
				tempTextInput.percentWidth=100;
				tempTextInput.height=aboutheight;
				tempTextInput.editable=false;
				tempTextInput.setStyle("borderColor","red");
				tempTextInput.setStyle("color","red");
				tempTextInput.text=errorMsg;
				this.errormsgbox.addChild(tempTextInput);
			}
			//得到保存数据
			protected function getSaveOrUpdatetData():String{
				var userid:String=FlexGlobals.topLevelApplication.userVo.id.toString();
				var dataJson:Object=new Object();
				if(ASStringHelper.isNullOrEmpty(this.dataId)){
					dataJson.INPUTUSER=userid;
				}
				dataJson.zcid=this.dataId;
				dataJson.xh=this.xh.text;
				dataJson.fyid=this.fyxh.data;
				dataJson.djid= (this.djxh.data == null ? this.djxh.text : this.djxh.data);
				
				dataJson.jqfs=this.jqfs.text;
				dataJson.fslx=this.fslx.text;
				dataJson.syzt=this.syzt.text;
				
				dataJson.zl=this.zl.text;
				dataJson.wxcc=this.wxcc.text;
				dataJson.sycx=this.sycx.text;
				
				dataJson.memo=this.memo.text;
				dataJson.UPDATEUSER=userid;
				
				/* dataJson.assettablename = jqfs.data;
				dataJson.assetidcolumn = ypsm.data;
				dataJson.assetid = ypfb.data; */
				return JSON.encode(dataJson);
			}
			//保存数据
			protected function dataSaveOrUpdateAction():void{
				if(!beforeSaveData()){
					return;
				}
				dataSaveOrUpdate();
			}
			
			public function dataSaveOrUpdate():void{
				var submitjson:String=getSaveOrUpdatetData();
				AllAssetServiceRmt.SaveOrUpdateObject(submitjson,"net.chinanets.pojos.ShryZcData","zcid","总成数据");
				AllAssetServiceRmt.addEventListener(ResultEvent.RESULT,dataSaveOrUpdateActionBack);
			}
			//保存数据回调方法
			protected function dataSaveOrUpdateActionBack(rmtResult:ResultEvent):void{
				AllAssetServiceRmt.removeEventListener(ResultEvent.RESULT,dataSaveOrUpdateActionBack);
				var doResult:DoResult=ASJsonHelper.stringJsonConverToDoResult(rmtResult.result.toString());
				if(doResult!=null && doResult.nRetCode!=Errors.OK){
					this.resultmsg.text=doResult.strErrorInfo;
				}else{
					this.resultmsg.text="数据操作成功";
					this.dataId=doResult.strErrorInfo;
					this.afterDoAction();
				}
				this.resultmsg.alpha=1;
				this.fadeEffect.stop();
				this.fadeEffect.play(null,false);
			}
			
			//删除当前数据
			protected function dataRemoveAction(doEvent:CloseEvent):void{
				if(doEvent.detail==Alert.CANCEL){
					return;
				}
				AllAssetServiceRmt.RemoveObject(this.dataId,"SHRY_ZC_DATA","ZCID","已选择数据记录");
				AllAssetServiceRmt.addEventListener(ResultEvent.RESULT,dataRemoveActionBack);
			}
			//删除当前数据回调方法
			protected function dataRemoveActionBack(rmtResult:ResultEvent):void{
				AllAssetServiceRmt.removeEventListener(ResultEvent.RESULT,dataRemoveActionBack);
				var doResult:DoResult=ASJsonHelper.stringJsonConverToDoResult(rmtResult.result.toString());
				if(doResult!=null && doResult.nRetCode!=Errors.OK){
					this.resultmsg.text=doResult.strErrorInfo;
					return;
				}else{
					this.resultmsg.text="数据删除成功";
					this.afterDoAction();
				}
			}
			//所有Action操作之后
			protected function afterDoAction():void{
				this.tempFunction.call(this);
			}
			
			//显示备注
			private function setMemoVisable():void{
				memolkb.visible=false;
				memolkb.includeInLayout=false;
				memolbl.visible=true;
				memolbl.includeInLayout=true;
				MEMOROW.height = memo.height=(aboutheight*3);
				datumFormTabNV.height += aboutheight;
			}
			
			var dhszsbl:Number = 0;
			var dhsdlhzj:Number = 0;
			public function getCbxData(hszsbl:Number,hsdlhzj:Number):void{
				if(CommonMethod.isNullOrWhitespace(dataId)){
					Alert.show("曲线表关联的表字段数据为空！",CommonXMLData.Alert_Title);
					return;
				}
				dhszsbl = hszsbl;  dhsdlhzj=hsdlhzj;
				
				//获取转速数据 到hsCbx下拉列表中显示 TODO 
				//	var sSqlZs:String = "select DISTINCT(lxdid) as lxdid,zzs from shry_fyxn_data fyxn where fyxn.fyid='"+dataId+"' ";
				var sSqlZs:String = "SELECT  symd,syrq,lxdid  FROM SHRY_SYD_DATA syd WHERE syd.zcid='"+dataId+"' ";
				
				commonService.RunSelectJSONArrayBySql(sSqlZs,null);
				commonService.addEventListener(ResultEvent.RESULT,getZsResultBack);
			}
			
			private function getZsResultBack(event:ResultEvent):void {
				commonService.removeEventListener(ResultEvent.RESULT,getZsResultBack);
				/* var strresult:String=event.result.toString();
				if(strresult == "" || !strresult.indexOf("{") == 0){ return; }
				var objList:ArrayCollection = JSON.decode(strresult,true) as ArrayCollection; */
				var objList:ArrayCollection = event.result as ArrayCollection;
				hsCbx.dataProvider = objList;
				hsCbx.selectedIndex = 0;
				
				getChartDataAndAddChart(dhszsbl,dhsdlhzj);
			}
			/**
			 * 获取图标信息
			 * */
			public function getChartDataAndAddChart(hszsbl:Number,hsdlhzj:Number):void{
				if(hsCbx.selectedItem == null ){ return; }
				var sLxdid:String = hsCbx.selectedItem.lxdid;
				//var sSql:String = "select * from SHRY_ZCXN_DATA where zcid = '"+dataId+"' order by (ll+0) asc";
				var sSql:String = "select * from SHRY_ZCXN_DATA where zcid = '"+dataId+"' and lxdid='"+sLxdid+"' order by (ll+0) asc";
				
				chartService.getZCXNChartList(sSql,dataId,hszsbl,hsdlhzj);  
				chartService.addEventListener(ResultEvent.RESULT,resultBack);
			}
			private function resultBack(event:ResultEvent):void {
				chartService.removeEventListener(ResultEvent.RESULT,resultBack);
				if(event.result == null)return;
			/* 	<shryChart:LinueChartMultiModel modelTitle="总成流量—静压/输入功率/主转速/效率曲线图"
				xAxisTitle="流量(m^3/h)" xToolTipTiltle="流量" yAxisTitle="静压/输入功率/主转速/效率" yToolTipTiltle="Y"
				serialLineName1="静压*(Pa)"  serialLineName2="输入*(W)"  serialLineName3="主转速*(r/min)"  serialLineName4="效率*(%)" 
				height="{this.height - 140}" id="lineChart" /> */
				//添加多曲线
				chartMultiPanel.removeAllChildren();
				var objList:ArrayCollection = event.result as ArrayCollection;
				chartDataList = objList;
				
				var objList2:ArrayCollection = ObjectUtil.copy(event.result) as ArrayCollection;
				var multiChart:LinueChartMultiModel = new LinueChartMultiModel();
				multiChart._yField3 = "zzs"; multiChart._yField2 = "srgl";
				multiChart.modelTitle="总成流量—静压/输入功率/转速/效率曲线图";  multiChart.xAxisTitle="流量(m^3/h)"; multiChart.xToolTipTiltle="流量";
				multiChart.yAxisTitle="静压/输入功率/转速/效率"; multiChart.yToolTipTiltle="Y";
				multiChart.serialLineName1="静压*(Pa)";  multiChart.serialLineName2="输入*(W)";  multiChart.serialLineName3="转速*(rpm)";  multiChart.serialLineName4="效率*(%)" ;
				multiChart.height=this.height - 140;
				multiChart.dpArray = objList2;
				chartMultiPanel.addChild(multiChart);
				
				/* <shryChart:LinueChartModel modelTitle="总成流量—效率曲线图" height="{this.height - 140}" id="lineChart02" />
					<shryChart:LinueChartModel modelTitle="总成流量—主转速曲线图" height="{this.height - 140}" id="lineChart03" />
					<shryChart:LinueChartModel modelTitle="总成流量—输入功率曲线图" height="{this.height - 140}" id="lineChart04" />
					<shryChart:LinueChartModel modelTitle="总成流量—静压曲线图" height="{this.height - 140}" id="lineChart05" /> */
				//添加单曲线
				chartPanel.removeAllChildren();
				var lineChart1:LinueChartModel = new LinueChartModel();
				lineChart1.modelTitle="总成流量—效率曲线图"; lineChart1.height=this.height - 140; 
				lineChart1._xField = "ll"; lineChart1._yField = "xl";
				lineChart1.xToolTipTiltle = lineChart1.xAxisTitle= "流量";lineChart1.yToolTipTiltle= lineChart1.yAxisTitle= "效率";
				lineChart1.dpArray = objList;
				var lineChart2:LinueChartModel = new LinueChartModel();
				lineChart2.modelTitle="总成流量—转速曲线图"; lineChart2.height=this.height - 140; 
				lineChart2._xField = "ll"; lineChart2._yField = "zzs";
				lineChart2.xToolTipTiltle = lineChart2.xAxisTitle= "流量";lineChart2.yToolTipTiltle= lineChart2.yAxisTitle= "转速";
				lineChart2.dpArray = objList;
				var lineChart3:LinueChartModel = new LinueChartModel();
				lineChart3.modelTitle="风叶流量—输入功率曲线图"; lineChart3.height=this.height - 140;
				lineChart3._xField = "ll"; lineChart3._yField = "srgl";
				lineChart3.xToolTipTiltle = lineChart3.xAxisTitle= "流量";lineChart3.yToolTipTiltle= lineChart3.yAxisTitle= "功率";
				lineChart3.dpArray = objList;
				var lineChart4:LinueChartModel = new LinueChartModel();
				lineChart4.modelTitle="风叶流量—静压曲线图"; lineChart4.height=this.height - 140;
				lineChart4.xToolTipTiltle = lineChart4.xAxisTitle= "流量";lineChart4.yToolTipTiltle= lineChart4.yAxisTitle= "静压";
				lineChart4._xField = "ll"; lineChart4._yField = "jyl";
				lineChart4.dpArray = objList;
				
				//添加数据表格
				var dataGrid:Grid = CommonUIMethod.generateTable("zcxn",objList);
				var vbx:VBox = new VBox();
				vbx.setStyle("horizontalAlign","center");	vbx.setStyle("borderColor","#99CCFF"); vbx.setStyle("borderStyle","solid");  
				vbx.percentWidth = 100;
				var hbx:HBox = new HBox();
				hbx.percentWidth = 100;
				hbx.height = 20;
				hbx.setStyle("borderStyle","none"); hbx.setStyle("backgroundColor","#99CCFF"); 
				hbx.setStyle("horizontalGap","0"); hbx.setStyle("verticalAlign","middle"); hbx.setStyle("verticalGap","0");
				var hbxLabel:Label = new Label(); hbxLabel.text="性能数据表";hbxLabel.height = 20; hbx.setStyle("fontWeight","bold"); hbx.setStyle("fontSize","13");
				hbx.addChild(hbxLabel);
				vbx.addChild(hbx);
				vbx.addChild(dataGrid);
				
