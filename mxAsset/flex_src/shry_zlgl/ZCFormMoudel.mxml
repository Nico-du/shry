				dataJson.gzdjy2= this.gzdjy2.text;
				dataJson.gzdll2= this.gzdll2.text;
				dataJson.gzdjy3= this.gzdjy3.text;
				
				dataJson.gzdll3= this.gzdll3.text;
				dataJson.zszt=   this.zszt.text;
				dataJson.zsgsed= this.zsgsed.text;
				dataJson.zsgssc= this.zsgssc.text;
				dataJson.zsdsed= this.zsdsed.text;
				
				dataJson.zsdssc= this.zsdssc.text;
				dataJson.phjph=  this.phjph.text;
				dataJson.phdph=  this.phdph.text;
				
				dataJson.memo=this.memo.text;
				dataJson.UPDATEUSER=userid;
				
				/* dataJson.assettablename = jqfs.data;
				dataJson.assetidcolumn = ypsm.data;
