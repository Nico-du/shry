				this.whkczs3.text=tempObj["whkczs3"];
				this.whkcdl3.text=tempObj["whkcdl3"];
				
				this.gzdjy1.text= tempObj["gzdjy1"];
				this.gzdll1.text= tempObj["gzdll1"];
				this.gzdjy2.text= tempObj["gzdjy2"];
				this.gzdll2.text= tempObj["gzdll2"];
				this.gzdjy3.text= tempObj["gzdjy3"];
				
				this.gzdll3.text= tempObj["gzdll3"];
				this.zszt.text=   tempObj["zszt"];
				this.zsgsed.text= tempObj["zsgsed"];
				this.zsgssc.text= tempObj["zsgssc"];
				this.zsdsed.text= tempObj["zsdsed"];
				
				this.zsdssc.text= tempObj["zsdssc"];
				this.phjph.text=  tempObj["phjph"];
				this.phdph.text=  tempObj["phdph"];
				
				this.dqdlhzjinput.text=tempObj["dlhzj"];
				this.hsdlhzjinput.text=tempObj["dlhzj"];
				
				/* jqfs.data=tempObj["assettablename"];
				ypsm.data=tempObj["assetidcolumn"];
				ypfb.data=tempObj["assetid"]; */
				
				if(!CommonMethod.isNullOrWhitespace(this.memo.text) && memolkb.visible){	setMemoVisable();}
