			public function getCbxData(hszsbl:Number,hsdlhzj:Number):void{
				if(CommonMethod.isNullOrWhitespace(dataId)){
					Alert.show("曲线表关联的表字段数据为空！",CommonXMLData.Alert_Title);
					return;
				}
				dhszsbl = hszsbl;  dhsdlhzj=hsdlhzj;
				
				//获取转速数据 到hsCbx下拉列表中显示 TODO 
			//	var sSqlZs:String = "select DISTINCT(lxdid) as lxdid,zzs from shry_fyxn_data fyxn where fyxn.fyid='"+dataId+"' ";
				var sSqlZs:String = "SELECT lxdid,zzs FROM shry_fyxn_data fyxn WHERE fyxn.fyid='"+dataId+"' GROUP BY lxdid";
				
				commonService.RunSelectJSONArrayBySql(sSqlZs,null);
				commonService.addEventListener(ResultEvent.RESULT,getZsResultBack);
