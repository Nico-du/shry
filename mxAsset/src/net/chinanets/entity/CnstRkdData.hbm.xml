<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.chinanets.entity.CnstRkdData" table="CNST_RKD_DATA" schema="ZCGL">
        <id name="rkdid" type="java.lang.String">
            <column name="RKDID" length="100" />            
        </id>
        <property name="rkdbh" type="java.lang.String">
            <column name="RKDBH" length="200" />
        </property>
        <property name="jhdw" type="java.lang.String">
            <column name="JHDW" length="200" />
        </property>
        <property name="fphm" type="java.lang.String">
            <column name="FPHM" length="1000" />
        </property>
        <property name="rkdd" type="java.lang.String">
            <column name="RKDD" length="100" />
        </property>
        <property name="rkrq" type="java.util.Date">
            <column name="RKRQ" length="7" not-null="true" />
        </property>
        <property name="usable" type="java.lang.Integer">
            <column name="USABLE" precision="10" scale="0"/>
        </property>
        <property name="ctime" type="java.util.Date">
            <column name="CTIME" length="7"/>
        </property>
        <property name="cuser" type="java.lang.String">
            <column name="CUSER" length="100"/>
        </property>
        <property name="utime" type="java.util.Date">
            <column name="UTIME" length="7"/>
        </property>
        <property name="uuser" type="java.lang.String">
            <column name="UUSER" length="100" />
        </property>
        <property name="memo" type="java.lang.String">
            <column name="MEMO" length="1000" />
        </property>
    </class>
</hibernate-mapping>
