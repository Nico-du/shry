<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="net.chinanets.pojos.CnstDatumData" table="CNST_DATUM_DATA">
        <id name="datumid" type="java.lang.String">
            <column name="DATUMID" length="100"/>
        </id>
        <property name="datumname" type="java.lang.String">
            <column name="DATUMNAME" length="200"/>
        </property>
        <property name="datumnameex" type="java.lang.String">
        	<column name="DATUMNAMEEX" length="200"/>
        </property>
        <property name="directoryid" type="java.lang.String">
            <column name="DIRECTORYID" length="100"/>
        </property>
        <property name="filesize" type="java.lang.Integer">
            <column name="FILESIZE"/>
        </property>
        <property name="filepath" type="java.lang.String">
            <column name="FILEPATH" length="500"/>
        </property>
        <property name="usable" type="java.lang.Integer">
            <column name="USABLE" length="10"/>
        </property>
        <property name="ctime" type="java.util.Date">
            <column name="CTIME"/>
        </property>
        <property name="cuser" type="java.lang.String">
            <column name="CUSER" length="100"/>
        </property>
        <property name="utime" type="java.util.Date">
            <column name="UTIME"/>
        </property>
        <property name="uuser" type="java.lang.String">
            <column name="UUSER" length="100"/>
        </property>
        <property name="memo" type="java.lang.String">
            <column name="MEMO" length="1000"/>
        </property>
    </class>
</hibernate-mapping>